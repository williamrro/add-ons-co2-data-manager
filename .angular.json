{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
	  "close-mg-temp": {
		"root": "",
		"sourceRoot": "src",
		"projectType": "application",
		"prefix": "app",
		"architect": {
		  "build": {
			"builder": "@angular-devkit/build-angular:browser",
			"options": {
			  "outputPath": "dist",
			  "index": "src/index.html",
			  "main": "src/main.ts",
			  "polyfills": "src/polyfills.ts",
			  "tsConfig": "src/tsconfig.app.json",
			  "aot": false,
			  "assets": [
				"src/assets",
				"src/favicon.ico"
			  ],
			  "styles": [
				"src/styles/styles.scss"
			  ],
			  "scripts": []
			},
			"configurations": {
			  "prod": {
				"fileReplacements": [
				  {
					"replace": "src/environments/environment.ts",
					"with": "src/environments/environment.prod.ts"
				  }
				],
				"optimization": true,
				"outputHashing": "all",
				"sourceMap": false,
				"extractCss": true,
				"namedChunks": false,
				"aot": true,
				"extractLicenses": true,
				"vendorChunk": false,
				"buildOptimizer": true
			  },
			  "sit": {
				"fileReplacements": [
				  {
					"replace": "src/environments/environment.ts",
					"with": "src/environments/environment.sit.ts"
				  }
				]
			  },
			  "dev": {
				"fileReplacements": [
				  {
					"replace": "src/environments/environment.ts",
					"with": "src/environments/environment.ts"
				  }
				]
			  }
			}
		  },
		  "serve": {
			"builder": "@angular-devkit/build-angular:dev-server",
			"options": {
			  "browserTarget": "close-mg-temp:build"
			},
			"configurations": {
			  "production": {
				"browserTarget": "close-mg-temp:build:production"
			  },
			  "sit": {
				"browserTarget": "close-mg-temp:build:sit"
			  },
			  "dev": {
				"browserTarget": "close-mg-temp:build:dev"
			  }
			}
		  },
		  "test": {
			"builder": "@angular-devkit/build-angular:karma",
			"options": {
			  "main": "src/test.ts",
			  "polyfills": "src/polyfills.ts",
			  "tsConfig": "src/tsconfig.spec.json",
			  "karmaConfig": "./karma.conf.js",
			  "styles": [
				"src/styles/styles.scss"
			  ],
			  "scripts": [],
			  "assets": [
				"src/assets",
				"src/favicon.ico"
			  ]
			}
		  },
		  "lint": {
			"builder": "@angular-devkit/build-angular:tslint",
			"options": {
			  "tsConfig": [
				"src/tsconfig.app.json",
				"src/tsconfig.spec.json",
				"e2e/tsconfig.e2e.json"
			  ],
			  "exclude": [
				"**/node_modules/**"
			  ]
			}
		  },
		  "e2e": {
			"builder": "@angular-devkit/build-angular:protractor",
			"options": {
			  "protractorConfig": "./protractor.conf.js",
			  "devServerTarget": "close-mg-temp:serve"
			},
			"configurations": {
			  "production": {
				"devServerTarget": "close-mg-temp:serve:production"
			  },
			  "sit": {
				"devServerTarget": "close-mg-temp:serve:sit"
			  },
			  "dev": {
				"devServerTarget": "close-mg-temp:serve:dev"
			  }
			}
		  }
		}
	  }
	},
	"defaultProject": "close-mg-temp"
  }
  